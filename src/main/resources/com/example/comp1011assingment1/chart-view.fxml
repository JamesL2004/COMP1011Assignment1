<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.ScatterChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="chartView" prefHeight="800.0" prefWidth="1200.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.comp1011assingment1.ChartViewController">
   <children>
      <VBox alignment="CENTER" prefHeight="800.0" prefWidth="1200.0" spacing="30.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label id="titleLabel2" text="The 35 Most Popular Streaming Services" />
            <ScatterChart fx:id="scatterChart" prefHeight="552.0" prefWidth="1140.0" VBox.vgrow="ALWAYS">
              <xAxis>
                <CategoryAxis id="titleAxis" label="Streaming Services" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis id="numAxis" label="# of Subscribers" side="LEFT" />
              </yAxis>
            </ScatterChart>
            <Button mnemonicParsing="false" onAction="#changeToTableView" text="Go To Table View" />
         </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
